    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build Successful'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Successful'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Successful'
            }
        }
    }
}
